server {
    listen 80;
    listen [::]:80;
    server_name note._DOMAIN_;
    root _STATICPATH_;
    index index.html;

    location / {
        add_header 'strict-transport-security' 'max-age=31536000; includeSubDomains; preload';
        add_header 'cache-control' 'public, max-age=31536000';
        try_files $uri $uri/ /index.html;
    }
}